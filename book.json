{
    "gitbook": "3.0.x",
    "plugins": [
        "-sharing",
        "edit-link@2.0.x",
        "github@2.0.x",
        "anchorjs"
    ],
    "pluginsConfig": {
        "edit-link": {
            "base": "https://github.com/swaggo/swaggo.io/edit/master/README.md",
            "label": "Edit"
        },
        "github": {
            "url": "https://github.com/swaggo/swag"
        },
        "anchorjs": {
            "icon": "#",
            "selector": "h1,h2,h3,h4,h5",
            "placement": "right",
            "visible": "always"
        }
    }
}
